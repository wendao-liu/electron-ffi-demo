{
  "name": "electron-ffi-demo",
  "version": "1.0.1",
  "description": "call a dll with ffi",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=development chcp 65001 &&  electron .",
    "build": "electron-builder 22.2.0",
    "start:node": "nodemon index.js"
  },
  "keywords": [],
  "author": "wzdxy",
  "license": "ISC",
  "build": {
    "asarUnpack": [
      "**/node_modules/**/*"
    ],
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "extraResources/icon.ico",
      "uninstallerIcon": "extraResources/icon.ico",
      "installerHeaderIcon": "extraResources/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "test"
    },
    "win": {
      "icon": "extraResources/icon.ico",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "extraFiles": [
      "dll",
      "extraResources",
      "node_modules"
    ]
  },
  "devDependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "electron": "^6.1.12",
    "electron-builder": "^21.2.0",
    "electron-reloader": "^1.2.1",
    "express": "^4.17.1",
    "ffi-napi": "^2.5.0",
    "fs": "0.0.1-security",
    "iconv-lite": "^0.6.3",
    "node-gyp": "^8.1.0",
    "nodemon": "^2.0.7",
    "path": "^0.12.7",
    "ref-napi": "^2.0.3",
    "socket.io": "^4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wendao-liu/electron-ffi-demo.git"
  },
  "bugs": {
    "url": "https://github.com/wendao-liu/electron-ffi-demo/issues"
  },
  "homepage": "https://github.com/wendao-liu/electron-ffi-demo#readme"
}
